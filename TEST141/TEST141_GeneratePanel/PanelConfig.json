{
    // There can be C-like comments
    "layout": {
        "type": "grid",
        "rows": 2,
        "cols": 3,
        "space": "2.4mm",
        "rotation": "0deg",
        "alternation": "none",
        "hbackbone": "3mm",
        "vbackbone": "3mm",
        "hboneskip": "0",
        "vboneskip": "0"
    },
    "source": {
        "type": "auto",
        "tolerance": "15mm"
    },
    "tabs": {
        "type": "annotation"
        // "width": "3mm",
        // "hcount": "1",
        // "vcount": "1",
        // "source": "MainBoard"
    },
    "cuts": {
        "type": "mousebites",
        "drill": "0.8mm",
        "spacing": "2.2mm",
        "offset": "0mm",
        "prolong": "0mm"

    },
    "framing": {
        "type": "tightframe",
        "width": "15mm",
        "slotwidth": "2.4mm",
        "space": "2.4mm"
        
    },
    "post": {
        "type": "auto",
        "millradius": "1.2mm"
    },
    "copperfill": {
        "type": "hatched",
        "clearance": "2.4mm",
        "spacing": "0.5mm",
        "width": "0.5mm"
    }
}
